{
  "name": "gameplayjdk/php-file-cache",
  "description": "Low-level, file based cache using var_export() and some magic. Optionally PSR-16 compliant.",
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "GameplayJDK",
      "email": "github@gameplayjdk.de"
    }
  ],
  "require": {
    "php": ">= 7.1",
    "symfony/filesystem": "^5.0",
    "psr/simple-cache": "^1.0",
    "brick/varexporter": "^0.3.2",
    "gameplayjdk/php-static-help": "^1.0"
  },
  "provide": {
    "psr/simple-cache-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Cache\\": "src/"
    }
  }
}
